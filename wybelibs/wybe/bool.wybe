pragma no_standard_library  # Standard library can't depend on itself!

pub constructors false | true

pub def `&&`(p:this, q:this):this = foreign llvm and(p,q)
pub def `||`(p:this, q:this):this = foreign llvm or(p,q)
pub def `~`(p:this):this = foreign llvm xor(p,true)
pub def =(x:this, y:this):this = foreign llvm icmp_eq(x,y)
pub def /=(x:this,y:this):this = foreign llvm icmp_ne(x,y)
pub def otherwise { } # not a test, empty body, so always true
pub def succeed { } # always succeed
