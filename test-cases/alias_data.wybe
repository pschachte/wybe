use student

public proc backup(student1:student, ?student2:student) use !io
  ?student2 = student1
end

public proc bar use !io
  ?course1 = course(101, "intro to cs")
  ?student1 = student(9401, course1)
  !backup(student1, ?student2)

  !println("student1")
  !printStudent(student1)

  !println("student2")
  !printStudent(student2)
end

!bar
