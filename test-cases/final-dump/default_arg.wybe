# Functions
def foo(a:int,b:int=1) = a+b
def bar(x:int,y=2) = x+y          # untyped optional
def baz(x:int,y:int=x+1) = x+y    # using mandatory arg in default
def qux(x:int,y=x,z=y+1) = x+y+z  # using earlier optional arg in default

# Procecures
def pfoo(a:int, b:int=1, ?r:int)  { ?r = a+b }
def pbar(x:int, y=2, ?r:int)      { ?r = x+y }
def pbaz(x:int,y:int=x+1, ?r:int) { ?r = x+y }
def pqux(x:int,y=x,z=y+1,?r:int)  { ?r = x+y+z }
