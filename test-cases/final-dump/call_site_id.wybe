
!read(?x:int)
!read(?y:int)


if { x > 0 :: 
    !foo(x)
    if { y > 0 ::
        !foo(y)
    | otherwise ::
        !foo(-y)
    }
| otherwise :: 
    !foo(-x)
    if { y > 0 ::
        !foo(y)
    | otherwise ::
        !foo(-y)
    }
}



