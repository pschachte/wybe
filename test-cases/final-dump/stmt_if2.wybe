# if case

pub type tree { pub empty | node(left:tree, key:int, right:tree) }

pub def lookup(key:int, tree:tree):bool =
    if {tree = empty:: false
       |tree = node(?l, ?k, ?r)::
            if {key = k:: true
               |key < k:: lookup(key, l)
               |else:: lookup(key, r)
            }
       |else:: false
    }

?tr = node(empty, 1, empty)

if{ lookup(1, tr) :: !print("found") }
