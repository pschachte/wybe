pub constructors(?T) nil | cons(car: ?T, cdr:this(?T))

pub def append(x:this(?T), y:this(?T)):this(?T) =
    if x = cons(?h, ?t)
    then cons(h, append(t,y))
    else y

pub def length(x:this(?T)) : int = length1(x, 0)

def length1(x:this(?T), acc:int) : int =
    if x = cons(?h, ?t) then length1(t, acc+1) else acc
