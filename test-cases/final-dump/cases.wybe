# Tests for case statement

def len(lst:list(T)):int = length where {
    case lst in {
        [_|?t]   :: ?length = 1 + len(t)
      | otherwise:: ?length = 0
    }
}

# Tests for case expression

def len2(lst:list(T)):int =
    case lst in {
        [_|?t]   :: 1 + len2(t)
      | otherwise:: 0
    }

