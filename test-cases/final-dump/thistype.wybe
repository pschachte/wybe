# test case for special `this` type

pub constructors nil | cons(head:int, tail:this)

pub def concat(x:this, y:this):this =
    if x = cons(?h, ?t)
    then cons(h,concat(t,y))
    else y

pub def length(x:this):int =
    if x = cons(?h, ?t) then 1 + length(t) else 0


?x = cons(1,cons(2,cons(3,nil)))
?y = cons(100, cons(200,nil))
!println(length(concat(x,y)))
