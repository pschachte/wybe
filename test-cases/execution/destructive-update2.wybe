# related to issue: https://github.com/pschachte/wybe/issues/21

pub type intlist { pub nil | cons(head:int, tail:intlist) }

pub def list_print(x:intlist) use !io {
    if { x = cons(?h, ?t) ::
        !print(h)
        !print(" ")
        !list_print(t)
    }
}

pub def list_println(x:intlist) use !io {
    !list_print(x)
    !println("")
}

?x = cons(1, cons(2, cons(3, cons(4, cons(5, cons(6, nil))))))
?y = cons(999, nil)
!list_println(x)
!list_println(y)

if { x = cons(?h, ?t) ::
    if { tail(!t, y) ::
        !print("t: ")
        !list_println(t)
    }
}

!list_println(x)
