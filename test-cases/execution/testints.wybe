def test_count(x:count, y:count) use !io {
    ?z = x * y
    !println("x = $x, y = $y, z = $z")
    !println("bound_minimum(x) = $(bound_minimum(x))")
    !println("bound_maximum(x) = $(bound_maximum(x))")
    !println("x + y = $(x+y)")
    if {x + ?y1 = z :: !println("$x + $y1 = $z") | else :: !println("$x + ?y1 = $z FAILS")}
    if {?x1 + y = z :: !println("$x1 + $y = $z") | else :: !println("?x1 + $y = $z FAILS")}
    ?dif = ("$(x - y)" | "FAIL")
    !println("x - y = $dif")
    if {z - ?x1 = y :: !println("$z - $x1 = $y") | else :: !println("$z - ?x1 = y FAILS") }
    ?z1 - x = y
    !println("$z1 - $x = $y")
    !println("x * y = $(x * y)")
    !println("x / y = $(x / y)")
    !println("x % y = $(x % y)")
    !println("x << y = $(x << y)")
    !println("z >> 2:count = $(z >> 2:count)")
    !println("x = y is $(x = y)")
    !println("x ~= y is $(x ~= y)")
    !println("x < y is $(x < y)")
    !println("x <= y is $(x <= y)")
    !println("x > y is $(x > y)")
    !println("x >= y is $(x >= y)")
    !println("x <=> y is $(x <=> y)")
    !println("x <=> (y+1:count) is $(x <=> (y+1:count))")
    !println("y <=> x is $(y <=> x)")
    !println("x = (y+1:count) is $(x = (y+1:count))")
    !println("x ~= (y+1:count) is $(x ~= (y+1:count))")
    !println("x < (y+1:count) is $(x < (y+1:count))")
    !println("x <= (y+1:count) is $(x <= (y+1:count))")
    !println("x > (y+1:count) is $(x > (y+1:count))")
    !println("x >= (y+1:count) is $(x >= (y+1:count))")
    !println("max(x,y) is $(max(x,y))")
    !println("min(x,y) is $(min(x,y))")
    !println("int(bound_maximum(x)) is $(int(bound_maximum(x)))")
    !println("int(bound_minimum(x)) is $(int(bound_minimum(x)))")
    !println("sqrt(z) is $(sqrt(z))")
    !println("x ** y = $(x ** y)")
}


