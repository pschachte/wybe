def add_one(i:int):int = i + 1
def add_one(f:float):float = f + 1.0

def {noinline} app(f:(I, ?J), i:I):J = f(i)

!println(app({@=?@}, 1.0))

!println(app({@+0.0=?@}, 1.0)) # checking flattening of @ works well

?y = 0.5
!println(app({@=?@+y}, 1.0))

?identity = id
!println(app(identity, 1.0))

!println(app(add_one, 1))

!println(app(`+`(10), 1))

use logging
!foo('a', true, 'b', 3)
def {noinline} foo(w: char, x:bool, y:char, z:int) use !io {
    !println(app({
        !logmsg(w)
        !logmsg(x)
        !logmsg(y)
        !logmsg(z)
        !lognl;
        @ + z = ?@
    }, 100))
}
